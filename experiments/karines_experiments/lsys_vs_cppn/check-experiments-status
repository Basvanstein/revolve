#!/bin/bash

runs=20
experiments=("plasticoding" "hyperplasticoding")



for i in $(seq $runs)
do
    run=$(($i))

    for experiment in "${experiments[@]}"
    do

     echo ""
     i="${experiment}_${run}.log";
     value=$(grep "Exported snapshot" $i|tail -n1|sed -E "s/\[(.*),.*Exported snapshot ([0-9]+).*/\2 -> \1/g");
     echo $i;
     echo $value;

    done
done


